@article{vishwanath2002dirac,
	title = {Dirac {Nodes} and {Quantized} {Thermal} {Hall} {Effect} in the {Mixed} {State} of d-wave {Superconductors}},
	volume = {66},
	issn = {0163-1829, 1095-3795},
	url = {http://arxiv.org/abs/cond-mat/0104213},
	doi = {10.1103/PhysRevB.66.064504},
	abstract = {We consider the vortex state of d-wave superconductors in the clean limit. Within the linearized approximation the quasiparticle bands obtained are found to posess Dirac cone dispersion (band touchings) at special points in the Brillouin zone. They are protected by a symmetry of the linearized Hamiltonian that we call T\_Dirac. Moreover, for vortex lattices that posess inversion symmetry, it is shown that there is always a Dirac cone centered at zero energy within the linearized theory. On going beyond the linearized approximation and including the effect of the smaller curvature terms (that break T\_Dirac), the Dirac cone dispersions are found to acquire small gaps (0.5 K/Tesla in YBCO) that scale linearly with the applied magnetic field. When the chemical potential for quasiparticles lies within the gap, quantization of the thermal-Hall conductivity is expected at low temperatures i.e. kappa\_\{xy\}/T = n[(pi k\_B){\textasciicircum}2/(3h)] with the integer `n' taking on values n=+2, -2, 0. This quantization could be seen in low temperature thermal transport measurements of clean d-wave superconductors with good vortex lattices.},
	number = {6},
	urldate = {2026-01-05},
	journal = {Physical Review B},
	author = {Vishwanath, Ashvin},
	month = aug,
	year = {2002},
	note = {arXiv:cond-mat/0104213},
	pages = {064504},
	annote = {Comment: (23 pages in all [7 pages in appendices], 9 figures)},
	file = {Preprint PDF:/home/nepero27178/Zotero/storage/FNYG4B6X/Vishwanath - 2002 - Dirac Nodes and Quantized Thermal Hall Effect in the Mixed State of d-wave Superconductors.pdf:application/pdf;Snapshot:/home/nepero27178/Zotero/storage/TQ9DEGRY/0104213.html:text/html},
}

@article{senarath2025mixed,
